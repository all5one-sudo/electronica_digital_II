NUM EQU 0X30
R_PAR EQU 0XEE
D_PAR EQU 0X110
R_IMP EQU 0XDD
D_IMP EQU 0X191
TEMP EQU 0X21
V_CTTE EQU .8
CTTE EQU 0X20
 
    ORG 0X00
    
INICIO
    CLRF TEMP     
    BCF STATUS,C  
    MOVLW V_CTTE
    MOVWF CTTE
LOOP
    RRF NUM,F 
    BTFSC STATUS,C 
    INCF TEMP,F 
    DECFSZ CTTE,F
    GOTO LOOP
    
    BTFSS TEMP,0  
    GOTO PAR
    GOTO IMPAR
 
PAR
    MOVLW R_PAR
    BSF STATUS,RP1 
    MOVWF D_PAR
    GOTO $

IMPAR 
    MOVLW R_IMP
    BSF STATUS,RP0   
    BSF STATUS,RP1   
    MOVWF D_IMP
    GOTO $
    
    END
